{
  "contract_version": 2,
  "strategy_title": "Test Strategy",
  "business_objective": "Reduce churn",
  "canonical_columns": ["col_a", "col_b"],
  "column_roles": {
    "pre_decision": ["col_a"],
    "decision": ["decision"],
    "outcome": ["target"],
    "post_decision_audit_only": ["audit"]
  },
  "allowed_feature_sets": {
    "model_features": ["col_a"],
    "segmentation_features": ["col_b"],
    "forbidden_features": ["audit"]
  },
  "artifact_requirements": {
    "clean_dataset": {
      "required_columns": ["col_a", "col_b"],
      "output_path": "data/cleaned_data.csv",
      "manifest_path": "data/cleaning_manifest.json"
    },
    "required_files": [
      "data/cleaned_data.csv",
      "data/metrics.json",
      "data/alignment_check.json"
    ]
  },
  "required_outputs": [
    "data/metrics.json",
    "data/alignment_check.json",
    "data/scored_rows.csv"
  ],
  "evaluation_spec": {
    "objective_type": "classification"
  },
  "reviewer_gates": ["metrics_present"],
  "qa_gates": [
    {
      "name": "no_synthetic_data",
      "severity": "HARD",
      "params": {}
    }
  ],
  "cleaning_gates": [
    {
      "name": "required_columns_present",
      "severity": "HARD",
      "params": {}
    }
  ],
  "reporting_policy": {
    "sections": ["Executive Decision"],
    "slots": [
      {
        "id": "model_metrics",
        "mode": "required",
        "sources": ["data/metrics.json"]
      }
    ]
  }
}
